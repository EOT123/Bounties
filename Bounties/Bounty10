# count to 1,000 by any multiple
print('')
print('_____________________________________________')
print('Enter A Number For Their Multiples Up To 1000')
print('_____________________________________________')
print('')
while True:
    print("")
    num = int(input('Enter A Number: '))
    for i in range(0, 1001, num):
        print(i)


# Done
